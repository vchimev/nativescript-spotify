    },
    {
      "name": "Brad Martin",
      "email": "bradwaynemartin@gmail.com",
      "url": "https://github.com/bradmartin"
    }
  ],
  "homepage": "https://github.com/NathanWalker/nativescript-spotify",
  "readmeFilename": "README.md"
}
